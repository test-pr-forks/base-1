{
  "LABEL": {
    "name": "format",
    "color": "EEEEEE"
  },
  "CHECKS": {
    "prefixes": ["fix: ", "feat: "],
    "regexp": "(feat\\(.*\\))|(fix\\(.*\\)):",
    "regexpFlags": "i",
    "ignoreLabels" : ["dont-check-PRs-with-this-label", "meta"]
  },
  "MESSAGES": {
    "success": "All OK-base1",
    "failure": "Failing CI test-base1",
    "notice": "Warning: This is a notice-base1"
  }
}
